<%= form_for @pho do |f| %>
	
	<p><%= f.label :name %>
	<%= f.text_field :name %></p>

	<p><%= f.label :img_url %>
	<%= f.text_field :img_url %></p>
	
	<p><%= f.label :rating %>
	<%= f.text_field :rating %></p>
	
	<p><%= f.label :review %>
	<%= f.text_field :review %></p>	
	
	<%= f.submit %>
<% end %>